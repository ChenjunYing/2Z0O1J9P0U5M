                    <div class="card-body collapse in" aria-expanded="true">
                        <div class="card-block">
                              <div class="table-responsive">
                                <form action="./roombooking" method="get">
                                      <table class="table table-hover">
                                        <tbody>
                                          <th>可选房型</th>
                                          <th>可选数量</th>
                                          <th>入住时间</th>
                                          <th>退房时间</th>
                                        </tbody>
                                        <tr>
                                          <td>双人间</td>
                                          <td>                                           
                <select name="double">
                <option value="0">0</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
              </select>
             
                                          </td>
                <td>
                  
               <input type="date" name="chekin" value="2019-5-1"/>
             
                </td>
                 <td>
                  
               <input type="date" name="chekout" value="2019-5-1"/>
                </td>

                                        </tr>
                                        <tr>
                                          <td>单人间</td>
                                          <td>
              <select name="single">
                <option value="0">0</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
              </select>
             
                                          </td>
              
                                        </tr>
                                        <tr>
                                          <td>豪华间</td>
                                          <td>
                                           
              <select name="lux">
                <option value="0">0</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
              </select>
             
                                          </td>
                  
                                        </tr>
                                        <tr>
                      <td>收货地址</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="address" ></td>
                    </tr>
                                      </table>
                                 
            <div class="card-body">
            <br>
              <div class="form-actions center">
                                    <button class="btn btn-success btn-doc-header btn-lg btn-block" type="submit" id="booking">预订</button>
