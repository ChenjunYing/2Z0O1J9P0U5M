          <div class="card-body collapse in" aria-expanded="true">
            <div class="card-block">
              <form action="./hotel_infoNew" method="get">
              <div class="table-responsive">
                <table class="table table-hover">
                  <tbody>
                    <tr>
                      <td>酒店名称</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="hotelName" id="hotelName"></td>
                    </tr>
                    <tr>
                      <td>位置</td>
                      <td><input type="text" size="80" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="hotelLocation" id="hotelLocation"></td>
                    </tr>
                    <tr>
                      <td>星级</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="hotelStar" id="hotelStar"></td>
                    </tr>
                    <tr>
                      <td>评分</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="hotelScore" id="hotelScore"></td>
                    </tr>
                    <tr>
                      <td>价格</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="hotelPrice" id="hotelPrice"></td>
                    </tr>
                    <tr>
                      <td>房间余量</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="room" id="room"></td>
                    </tr>
                    <tr>
                      <td>房型：1-单人间 2-双人间 3-豪华间</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="type" ></td>
                    </tr>
                    <tr>
                      <td>酒店信息</td>
                      <td><input type="text" size="30" style="border: 1px solid gray;border-radius: 4px;padding:8px" required name="instr"></td>
                    </tr>
                  </tbody>
                </table>
                <button class="btn btn-success btn-doc-header btn-lg btn-block" type="submit" >确认</button>
              </div>
            </form>
