<div class="app-content content container-fluid">
  <div class="content-wrapper">
    <div class="content-body">

      

      <div class="card">

        <div class="card-header">
          <% if (details.length==0) { %>
            <h4 id="basic-forms" class="card-title">
            无记录</h4>
            <% } else { %>
            <h4 id="basic-forms" class="card-title">
            <%= details[0].created_time %></h4>
          <% } %>
          <div class="heading-elements">
            <ul class="list-inline mb-0">
             <li>input the check order id &nbsp </li>
             <li> <input name="key" type="text" id="key" onkeydown="onSearch(this)" class="input_test" value="" /></li>
             <li>&nbsp &nbsp &nbsp show mode &nbsp </li>
             <li><a href="javascript:void(0);" onclick="onSearchAll()" target="_blank" class="btn btn-primary">全部</a></li>
             <li><a href="javascript:void(0);" onclick="onSearchBtn(1)" target="_blank" class="btn btn-success">正确</a></li>
             <li><a href="javascript:void(0);" onclick="onSearchBtn(2)" target="_blank" class="btn btn-warning">已修改</a></li>
             <li><a href="javascript:void(0);" onclick="onSearchBtn(3)" target="_blank" class="btn btn-danger">待修改</a></li>
           </ul>
         </div>
       </div>

       <div class="card-body collapse in" aria-expanded="true">
        <div class="card-block">
          <div class="table-responsive" >

            <table class="table table-hover table-striped" id="order_list">

              <tbody>
                <th>流水号</th>
                <th>订单号</th>
                <th>资金流出方</th>
                <th>资金流入方</th>
                <th>金额</th>
                <th>订单信息</th>
                <th>流水创建时间</th>
                <th>核对状态</th>
                <% for (var i = 0; i < details.length; i++) { %>
                <tr id='testchooce'>
                  <% if (details[i].transaction_status==1) {%>
                  <td id="dealid"><font color="37BC9B"><%= details[i].deal_id %></font></td>
                  <% } else if (details[i].transaction_status==2) { %>
                  <td id="dealid"><font color="F6BB42"><%= details[i].deal_id %></font></td>
                  <% } else if (details[i].transaction_status==3) { %>
                  <td id="dealid"><font color="DA4453"><%= details[i].deal_id %></font></td>
                  <% } %>

                  <td id="orderid" onclick="update(this)" class="pig"><%= details[i].order_id %></td>
                  <td id="fromid"><%= details[i].from_id %></td>
                  <td id="toid"><%= details[i].to_id %></td>
                  <td id="amount"><%= details[i].amount %></td>
                  <td id="infomation"><%= details[i].infomation %></td>
                  <td id="createdtime"><%= details[i].created_time %></td>
                  <td id="transactionstatus"><%= details[i].transaction_status %></td>
                </tr>
                      <% } %>
              </tbody>

            </table>

          </div>
        </div>
      </div>

    </div>

  </div>
